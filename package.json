{
  "name": "@senzing/sdk-components-ng",
  "version": "1.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng build @senzing/sdk-components-ng && ng serve",
    "start:documentation": "./node_modules/.bin/compodoc -s -d docs",
    "start:docs": "npm run start:documentation",
    "build": "ng build @senzing/sdk-components-ng && node build-sdk.js",
    "build:docs": "./node_modules/.bin/compodoc -p projects/sdk-components-ng/tsconfig.lib.json -n \"Senzing SDK Components\" --includesName \"Guides\" -d docs -a projects/sdk-components-ng/docs/images --includes projects/sdk-components-ng/docs --disableInternal --disablePrivate --disableDomTree --disableGraph --disableRoutesGraph --disableCoverage --disableSourceCode && node build-docs",
    "build:example": "ng build --prod --output-hashing none",
    "clean": "rimraf dist/@senzing/sdk-components-ng",
    "package": "npm run build:docs && npm run build && cd ./dist/@senzing/sdk-components-ng/ && npm pack",
    "publish": "cd ./dist/@senzing/sdk-components-ng/ && npm publish --access restricted",
    "watch": "rm -fR dist/@senzing/sdk-components-ng && concurrently --kill-others \"wait-on file:dist/@senzing/sdk-components-ng/public_api.d.ts && ng serve\" \"ng build @senzing/sdk-components-ng --watch\"",
    "test": "ng test",
    "start:server": "java -jar \"../senzing-api-server/target/senzing-api-server-1.5.1.jar\" -iniFile \"%LOCALAPPDATA%\\Senzing\\Workbench\\project_1\\g2.ini\" -allowedOrigins http://localhost:4300",
    "start:rest": "npm run start:server",
    "server": "npm run start:server",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "example:search-with-results-and-details": "ng serve example/search-with-results-and-details",
    "example:search-with-spinner": "ng serve example/search-with-spinner",
    "example:graph": "ng serve example/graph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/elements": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@senzing/rest-api-client-ng": "^1.0.1",
    "@types/d3": "^5.7.1",
    "core-js": "^2.5.4",
    "d3": "^5.9.1",
    "document-register-element": "1.8.1",
    "http-server": "^0.11.1",
    "json-query-string": "^1.0.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "concurrently": "^4.0.1",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.11.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "scss-bundle": "^2.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "wait-on": "^3.2.0"
  }
}
