{
  "name": "@senzing/sdk-components-ng",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build @senzing/sdk-components-ng && ng serve",
    "start:documentation": "./node_modules/.bin/compodoc -s -d docs",
    "start:docs": "npm run start:documentation",
    "build": "ng build @senzing/sdk-components-ng && node build-sdk.js",
    "build:docs": "./node_modules/.bin/compodoc -p src/tsconfig.lib.json -n \"Senzing SDK Components\" --includesName \"Guides\" -d docs -a src/docs/images --includes src/docs --disableInternal --disablePrivate --disableDomTree --disableGraph --disableRoutesGraph --disableCoverage --disableSourceCode && node build-docs",
    "build:example": "ng build --prod --output-hashing none",
    "clean": "rimraf dist/@senzing/sdk-components-ng",
    "package": "cd ./dist/@senzing/sdk-components-ng/ && npm pack",
    "publish": "cd ./dist/@senzing/sdk-components-ng/ && npm publish --access restricted",
    "watch": "rm -fR dist/@senzing/sdk-components-ng && concurrently --kill-others \"wait-on file:dist/@senzing/sdk-components-ng/public_api.d.ts && ng serve\" \"ng build @senzing/sdk-components-ng --watch\"",
    "test": "ng test",
    "test:headless": "ng test --no-watch --watch=false --progress=false --no-progress --browsers=ChromeHeadlessCI",
    "start:server": "java -jar \"..\\senzing-api-server\\target\\senzing-api-server-1.7.2.jar\" -iniFile \"%LOCALAPPDATA%\\Senzing\\Workbench\\project_1\\g2.ini\" -httpPort 8080 -allowedOrigins \"*\"",
    "start:rest": "npm run start:server",
    "server": "npm run start:server",
    "lint": "ng lint",
    "e2e": "ng e2e @senzing/sdk-components-ng/e2e --configuration=e2e --protractor-config=e2e/protractor.conf.js",
    "e2e:headless": "ng e2e @senzing/sdk-components-ng/e2e --configuration=e2e --protractor-config=e2e/protractor.ci.conf.js",
    "example:search-with-results-and-details": "ng serve example/search-with-results-and-details",
    "example:search-with-spinner": "ng serve example/search-with-spinner",
    "example:graph": "ng serve example/graph",
    "example:search-with-prefs": "ng serve example/search-with-prefs",
    "example:search-in-graph": "ng serve example/search-in-graph",
    "watch:search-with-prefs": "rm -fR dist/@senzing/sdk-components-ng && concurrently --kill-others \"wait-on file:dist/@senzing/sdk-components-ng/public_api.d.ts && ng serve example/search-with-prefs\" \"ng build @senzing/sdk-components-ng --watch\"",
    "watch:search-in-graph": "rm -fR dist/@senzing/sdk-components-ng && concurrently --kill-others \"wait-on file:dist/@senzing/sdk-components-ng/public_api.d.ts && ng serve example/search-in-graph\" \"ng build @senzing/sdk-components-ng --watch\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/elements": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@senzing/rest-api-client-ng": "^1.0.2",
    "@senzing/sdk-graph-components": "file:../sdk-graph-components/dist/@senzing/sdk-graph-components/senzing-sdk-graph-components-0.1.0.tgz",
    "core-js": "^2.5.4",
    "d3": "^5.9.1",
    "document-register-element": "1.8.1",
    "html2canvas": "^1.0.0-rc.3",
    "http-server": "^0.11.1",
    "json-query-string": "^1.0.0",
    "jspdf": "^1.5.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular-devkit/build-ng-packagr": "^0.12.4",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@compodoc/compodoc": "^1.1.10",
    "@types/d3": "^5.7.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jspdf": "^1.3.0",
    "@types/node": "~8.9.4",
    "angular-in-memory-web-api": "^0.8.0",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "concurrently": "^4.0.1",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.11.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "sass-burger": "^1.3.1",
    "scss-bundle": "^2.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "wait-on": "^3.2.0"
  },
  "optionalDependencies": {
    "ngx-webstorage-service": "^4.1.0"
  }
}
